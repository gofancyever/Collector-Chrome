{
    "name": "Kallax",
    "description": "保存到Kallax",
    "version": "1.0",
    "background": {
        "scripts": ["axios.min.js","background-script.js","contextMenus.js"]
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*","cookies","notifications","storage","contextMenus"
    ],
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
      },
    "browser_action": {
        "default_title": "Kallax",
        "default_icon": "logo2.png"
    },
    "manifest_version": 2
}